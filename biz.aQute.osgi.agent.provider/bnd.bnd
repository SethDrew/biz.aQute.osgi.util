Bundle-Version:					1.0.0.${tstamp}
Bundle-Activator:               biz.aQute.osgi.agent.provider.Activator

Bundle-Description: \
	Implements a simple agent to update/install/uninstall bundles

-conditionalpackage: aQute.lib*

Private-Package: \
